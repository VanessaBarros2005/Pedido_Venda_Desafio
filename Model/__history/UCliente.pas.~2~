unit UCliente;

interface

uses
  System.SysUtils;

type
  TCliente = class
  private
    FID_Cliente: Integer;
    FNome: string;
    FCidade: string;
    FUF: string;
  public
    property ID_Cliente: Integer read  FID_Cliente write FID_Cliente;
    property Nome: string read FNome write FNome;
    property Cidade: string read FCidade write FCidade;
    property UF: string read FUF write FUF;
  end;

implementation

end.
